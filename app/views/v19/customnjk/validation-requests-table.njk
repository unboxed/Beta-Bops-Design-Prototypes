{% macro requestsTable(action1, action2, action3, action4) %}
{% block content %}


<div> 
    <h1 class="govuk-heading-l">
        Validation requests
    </h1>
</div>
<div class="govuk-grid-row">
    <div class="govuk-grid-column-full">
        <table class="govuk-table">
            <thead class="govuk-table__head">
                <tr class="govuk-table__row">
                <th scope="col" class="govuk-table__header">Request</th>
                <th scope="col" class="govuk-table__header">Detail</th>
                <th scope="col" class="govuk-table__header">Status</th>
                <th scope="col" class="govuk-table__header">View request</th>
                </tr>
            </thead>
            <tbody class="govuk-table__body">
                <tr class="govuk-table__row">
                    <th scope="row" class="govuk-table__header">Red line boundary change</th>
                    <td class="govuk-table__cell"><a class="govuk-link" href="table-redline.html">View proposed red line boundary</a></td>
                    <td class="govuk-table__cell"><strong class="govuk-tag govuk-tag--green">sent</strong></td>
                    <td class="govuk-table__cell"><a class="govuk-link" href="#">View request</a></td>
                </tr>
                <tr class="govuk-table__row">
                    <th scope="row" class="govuk-table__header"><a class="govuk-link" href="table-replacement.html">Replacement document </a></th>
                    <td class="govuk-table__cell">Section_proposed.pdf</td>
                    <td class="govuk-table__cell"><strong class="govuk-tag govuk-tag--red">overdue</strong></td>
                    <td class="govuk-table__cell"><a class="govuk-link" href="#">View request</a></td>
                </tr>
                <tr class="govuk-table__row">
                    <th scope="row" class="govuk-table__header">New document request</th>
                    <td class="govuk-table__cell">[Document requested]</td>
                    <td class="govuk-table__cell"><strong class="govuk-tag govuk-tag--yellow">Not sent yet</strong></td>
                    <td class="govuk-table__cell"><a class="govuk-link" href="#">View request</a></td>
                </tr>
                <tr class="govuk-table__row">
                    <th scope="row" class="govuk-table__header">Other validation request</th>
                    <td class="govuk-table__cell">Test</td>
                    <td class="govuk-table__cell"><strong class="govuk-tag">responded</strong></td>
                    <td class="govuk-table__cell"><a class="govuk-link" href="#">View request</a></td>
                </tr>
                 <tr class="govuk-table__row">
                    <th scope="row" class="govuk-table__header">Other validation request</th>
                    <td class="govuk-table__cell">Test</td>
                    <td class="govuk-table__cell"><strong class="govuk-tag govuk-tag--grey">cancelled</strong></td>
                    <td class="govuk-table__cell"><a class="govuk-link" href="#">View request</a></td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        <div class="govuk-button-group">
        <a class="govuk-button govuk-button--primary" data-module="govuk-button" href="validate-tasklist-8.html">
            Back
        </a>
        </div>
    </div>
</div>

{% endblock %}

{% endmacro %}